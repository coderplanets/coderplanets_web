{
  "plugins": [
    "transform-decorators-legacy",
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "react-intl",
      {
        "messagesDir": "lang/.messages/"
      }
    ],
    [
      "import",
      {
        "libraryName": "antd"
      }
    ],
    "ramda"
  ],
  "env": {
     "development": {
       "presets": ["next/babel"],
       "plugins": ["inline-dotenv"]
    },
    "production": {
      "presets": ["next/babel"]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]],
      "plugins": ["inline-dotenv"]
    }
  }
}
